{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "DormWay Docs",
  "colors": {
    "primary": "#6366F1",
    "light": "#818CF8",
    "dark": "#4F46E5"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["index"]
          }
        ]
      },
      {
        "tab": "Engineering",
        "groups": [
          {
            "group": "Architecture",
            "icon": "sitemap",
            "pages": [
              "docs/engineering/architecture/sot-ios-widgetkit"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": ["api-reference/introduction"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/DormWay",
          "icon": "github"
        },
        {
          "anchor": "Linear",
          "href": "https://linear.app/dormway",
          "icon": "list-check"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Slack",
        "href": "https://dormway.slack.com"
      }
    ]
  },
  "contextual": {
    "options": ["copy", "view", "claude", "cursor", "vscode"]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/DormWay"
    }
  },
  "modeToggle": {
    "default": "dark"
  }
}
